    "react-dom": "19.0.0-rc-66855b96-20241106"
  },
  "devDependencies": {
    "@prisma/nextjs-monorepo-workaround-plugin": "^6.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
